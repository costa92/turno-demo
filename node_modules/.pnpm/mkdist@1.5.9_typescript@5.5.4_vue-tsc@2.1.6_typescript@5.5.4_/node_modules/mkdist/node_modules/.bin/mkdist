#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hellotalk/code/web/demo/node_modules/.pnpm/mkdist@1.5.9_typescript@5.5.4_vue-tsc@2.1.6_typescript@5.5.4_/node_modules/mkdist/dist/node_modules:/home/hellotalk/code/web/demo/node_modules/.pnpm/mkdist@1.5.9_typescript@5.5.4_vue-tsc@2.1.6_typescript@5.5.4_/node_modules/mkdist/node_modules:/home/hellotalk/code/web/demo/node_modules/.pnpm/mkdist@1.5.9_typescript@5.5.4_vue-tsc@2.1.6_typescript@5.5.4_/node_modules:/home/hellotalk/code/web/demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hellotalk/code/web/demo/node_modules/.pnpm/mkdist@1.5.9_typescript@5.5.4_vue-tsc@2.1.6_typescript@5.5.4_/node_modules/mkdist/dist/node_modules:/home/hellotalk/code/web/demo/node_modules/.pnpm/mkdist@1.5.9_typescript@5.5.4_vue-tsc@2.1.6_typescript@5.5.4_/node_modules/mkdist/node_modules:/home/hellotalk/code/web/demo/node_modules/.pnpm/mkdist@1.5.9_typescript@5.5.4_vue-tsc@2.1.6_typescript@5.5.4_/node_modules:/home/hellotalk/code/web/demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../dist/cli.cjs" "$@"
fi
